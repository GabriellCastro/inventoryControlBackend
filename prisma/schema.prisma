generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id          Int      @id @default(autoincrement())
  name        String
  description String
  quantity    Int      @default(0)
  price       Int
  image       String?
  createdAt   DateTime @default(now())
}
